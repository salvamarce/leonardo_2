<launch>
	<param name="use_sim_time" value="true" />

	<node pkg="rosbag" type="play" name="rosbag" required="true" args="-l $(find leonardo_2)/bag_files/primo_marker_2021-03-02-15-05-21.bag"/>

	<node name="republish" type="republish" pkg="image_transport" output="screen" args="compressed in:=/rs435d/color/image_raw raw out:=/rs435d/color/image_raw" />
	
	<node name="extract" pkg="image_view" type="extract_images" respawn="false" required="true" output="screen" cwd="ROS_HOME">
		<remap from="image" to="/aruco_marker_publisher/result"/>
	</node>

	<include file="$(find leonardo_2)/launch/aruco_detection.launch" /> 
	
	
</launch>
